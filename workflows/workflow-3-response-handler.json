      "id": "linq-webhook",
      "name": "Linq Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1.1,
      "position": [240, 400],
      "webhookId": "linq-response"
    },
    {
      "parameters": {
        "jsCode": "// Parse incoming Linq webhook\nconst body = $input.first().json.body || $input.first().json;\n\n// Linq webhook payload structure\nconst event = body.event || body.type || 'message.received';\nconst data = body.data || body;\n\n// Extract phone number (the lead's phone)\nlet phone = data.from_phone_number || data.from || data.sender_phone || '';\n\n// Normalize phone number\nphone = phone.replace(/[^0-9+]/g, '');\nif (phone && !phone.startsWith('+')) {\n  if (phone.length === 10) {\n    phone = '+1' + phone;\n  } else if (phone.length === 11 && phone.startsWith('1')) {\n    phone = '+' + phone;\n  }\n}\n\n// Extract message content\nconst messageText = data.message?.text || data.text || data.body || '';\n\nreturn [{\n  json: {\n    event: event,\n    phone: phone,\n    message_text: messageText,\n    message_preview: messageText.substring(0, 100),\n    received_at: data.created_at || new Date().toISOString(),\n    linq_chat_id: data.chat_id || data.id || null,\n    raw_payload: body\n  }\n}];"
      },
      "id": "parse-linq-webhook",
      "name": "Parse Linq Webhook",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [460, 400]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "has-phone",
              "leftValue": "={{ $json.phone }}",
              "rightValue": "",
              "operator": {
                "type": "string",
                "operation": "notEmpty"
              }
            },
            {
              "id": "is-received",
              "leftValue": "={{ $json.event }}",
              "rightValue": "message.received",
              "operator": {
                "type": "string",
                "operation": "contains"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "filter-valid-response",
      "name": "Filter Valid Responses",
      "type": "n8n-nodes-base.filter",
      "typeVersion": 2,
      "position": [680, 400]
    },
