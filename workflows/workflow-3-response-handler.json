      "id": "filter-found",
      "name": "Filter Found Contacts",
      "type": "n8n-nodes-base.filter",
      "typeVersion": 2,
      "position": [1120, 400]
    },
    {
      "parameters": {
        "jsCode": "// Extract contact info from search results\nconst searchResult = $input.first().json;\nconst webhookData = $('Parse Linq Webhook').first().json;\n\nconst contact = searchResult.results?.[0] || searchResult[0] || {};\nconst contactId = contact.id || contact.contactId;\n\nif (!contactId) {\n  return [{\n    json: {\n      error: 'No contact ID found',\n      phone: webhookData.phone\n    }\n  }];\n}\n\nconst properties = contact.properties || contact;\n\nreturn [{\n  json: {\n    contact_id: contactId,\n    first_name: properties.firstname || properties.first_name || 'Lead',\n    last_name: properties.lastname || properties.last_name || '',\n    email: properties.email || '',\n    phone: webhookData.phone,\n    message_text: webhookData.message_text,\n    message_preview: webhookData.message_preview,\n    received_at: webhookData.received_at,\n    hubspot_url: `https://app.hubspot.com/contacts/YOUR_PORTAL_ID/contact/${contactId}`\n  }\n}];"
      },
      "id": "extract-contact",
      "name": "Extract Contact Info",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1340, 400]
