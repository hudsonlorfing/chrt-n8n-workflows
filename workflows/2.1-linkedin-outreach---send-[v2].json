      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        4320,
        928
      ],
      "onError": "continueRegularOutput"
    },
    {
      "parameters": {
        "jsCode": "// Parse Apps Script response — extract the leads array into individual items\nconst resp = $input.first().json;\n\nif (!resp.ok) {\n  throw new Error('Apps Script error: ' + (resp.error || 'unknown'));\n}\n\nif (!resp.leads || resp.leads.length === 0) {\n  // Return empty to skip downstream nodes gracefully\n  return [];\n}\n\nconst profile = resp.profile || $('Calculate Send Limit').first().json.profile;\nreturn resp.leads.map(lead => ({ json: { ...lead, profile } }));"
      },
