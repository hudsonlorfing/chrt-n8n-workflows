    {
      "parameters": {
        "jsCode": "// Find new connections by comparing export results against pending leads\nconst exportItems = $('Fetch Connection Export JSON').all();\nconst pendingItems = $('Filter Pending (Sent, Not Connected)').all();\nconst context = $('Parse Webhook Context').first().json;\n\n// Build set of exported connection URLs (these are current connections)\nconst connectedUrls = new Set();\nexportItems.forEach(item => {\n  const url = (item.json.profileUrl || item.json.linkedinProfileUrl || '').trim().toLowerCase().replace('https://www.linkedin.com', 'https://linkedin.com').replace(/\\/$/, '');\n  if (url) connectedUrls.add(url);\n});\n\n// Find pending leads who are now connected\nconst newConnections = pendingItems.filter(item => {\n  const url = (item.json.defaultProfileUrl || item.json.profileUrl || '').trim().toLowerCase().replace('https://www.linkedin.com', 'https://linkedin.com').replace(/\\/$/, '');\n  return url && connectedUrls.has(url);\n});\n\nreturn [{ json: {\n  newConnectionCount: newConnections.length,\n  totalExported: exportItems.length,\n  totalPending: pendingItems.length,\n  profile: context.profile,\n  newConnections: newConnections.map(item => ({\n    profileUrl: item.json.defaultProfileUrl || item.json.profileUrl || '',\n    fullName: item.json.fullName || '',\n    company: item.json.company || ''\n  }))\n}}];"
      },
      "id": "wf3v2p-find-new",
      "name": "Find New Connections",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        4200,
        720
      ]
    },
    {
      "parameters": {
        "jsCode": "// Extract URLs of new connections for email scraper input\nconst data = $input.first().json;\nconst connections = data.newConnections || [];\nreturn connections.map(c => ({ json: { linkedInProfileUrl: c.profileUrl } }));"
      },
